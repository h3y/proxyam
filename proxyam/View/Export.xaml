<UserControl x:Class="proxyam.View.Export"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:proxyam.View"
             xmlns:viewModel="clr-namespace:proxyam.ViewModel"
             mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=viewModel:ExportViewModel}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel>
                <ItemsControl ItemsSource="{Binding Path=ProxyType}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <CheckBox Content="{Binding Path=Key}" IsChecked="{Binding Path=Value,Mode=TwoWay}"></CheckBox>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
            </StackPanel>
            <StackPanel>
                <Button Width="100">123</Button>
                <Button Width="100">123</Button>
                <ToggleButton Width="100">123</ToggleButton>
            </StackPanel>
        </StackPanel>

    </Grid>
</UserControl>
